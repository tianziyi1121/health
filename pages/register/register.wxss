.userData { width: 690rpx; margin: 0 auto ; margin-top: 40rpx;}
.userData .userName { margin: 40rpx 0 0 50rpx; z-index: 2;}
.userName image { width: 144rpx; height: 144rpx;}
.userName > view { margin: 26rpx 0 0 14rpx;}
.userName > view > view:nth-child(1) { color: #fff;}
.userName > view > view:nth-child(2) { color: #ffd200; margin-top: 30rpx;}
.userData > view:nth-child(2) { width: 690rpx;}
.userData > view:nth-child(2) image{ width: 100%; height: 277rpx;}
.userTitle { margin: 30rpx 0 26rpx 30rpx;}
.userList > view { width: 100%;}
.subset > view { width: 200rpx; height: 268rpx; border: 2rpx solid #37cf6b; border-radius: 10rpx;}
.subset > view > view:nth-child(1) { margin: 70rpx 0 16rpx 0;}
.subset > view > view:nth-child(3) { margin-top: 20rpx;}
.userPrice { color: #ffd200;}
.active { background: #ddffe9;}
.character image { width: 100%; height: 200rpx; margin-bottom: 40rpx;}
.userFooter { height: 100rpx; left: 0; bottom: 0; width: 100%;}
.userFooter > view { width: 100%; line-height: 100rpx;}
.userFooter > view:nth-child(1) { background: #fff; color: #ff0000;}
.userFooter > view:nth-child(2) { background: #37cf6b; color: #fff;}